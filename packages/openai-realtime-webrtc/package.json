{
  "name": "@openai-realtime/webrtc",
  "version": "4.0.2",
  "description": "Simple WebRTC module for OpenAI's Realtime API - Basic voice streaming for experiments and prototypes",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "MIGRATION.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:rollup",
    "build:rollup": "rollup -c rollup.config.mjs",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest src/__tests__/simple-memory-test.test.ts src/__tests__/mock-verification.test.ts --ci --watchAll=false --testTimeout=10000 --collectCoverage=false",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "clean": "rimraf dist docs coverage",
    "validate": "npm run test:ci",
    "prepack": "npm run validate && npm run build",
    "prepublishOnly": "npm run validate && npm run build",
    "prepublish:check": "node scripts/prepublish-check.js"
  },
  "keywords": [
    "openai",
    "realtime",
    "realtime-api",
    "webrtc",
    "audio",
    "audio-streaming",
    "voice",
    "voice-chat",
    "typescript",
    "session-management",
    "streaming",
    "react",
    "react-hooks",
    "audio-processing",
    "speech",
    "conversation",
    "ai",
    "gpt",
    "assistant"
  ],
  "author": "Spanish Tutor MVP",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@openai/realtime-api-beta": "github:openai/openai-realtime-api-beta",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spanish-tutor/spanish-tutor-mvp.git",
    "directory": "packages/openai-realtime-webrtc"
  },
  "bugs": {
    "url": "https://github.com/spanish-tutor/spanish-tutor-mvp/issues"
  },
  "homepage": "https://github.com/spanish-tutor/spanish-tutor-mvp/tree/main/packages/openai-realtime-webrtc#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/spanish-tutor"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  }
}
